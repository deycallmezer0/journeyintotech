{
    "version": 2,
    "builds": [
      {
        "src": "manage.py",
        "use": "@vercel/python",
        "config": {
          "installCommand": "pip install -r requirements.txt",
          "buildCommand": "python manage.py collectstatic --noinput"
        }
      },
      {
        "src": "staticfiles/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/manage.py"
      }
    ]
  }
  