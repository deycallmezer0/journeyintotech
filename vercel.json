{
    "version": 2,
    "builds": [
      {
        "src": "portfolio/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "installCommand": "pip install -r requirements.txt",
          "buildCommand": "python manage.py collectstatic --noinput"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/portfolio/wsgi.py"
      }
    ],
    "env": {
      "POSTGRES_URL": "@POSTGRES_URL",
      "SECRET_KEY": "@your-secret-key"
    }
  }
  