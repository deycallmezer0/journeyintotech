{
    "version": 2,
    "builds": [
      {
        "src": "manage.py",
        "use": "@vercel/python",
        "config": {
          "installCommand": "pip install -r requirements.txt",
          "buildCommand": "python manage.py collectstatic --noinput"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/manage.py"
      }
    ],
  "env": {
    "POSTGRES_URL": "@POSTGRES_URL",
    "SECRET_KEY": "@SECRET_KEY",
    "DEBUG": "@DEBUG",
    "ALLOWED_HOSTS": "@ALLOWED_HOSTS"
  }
}
